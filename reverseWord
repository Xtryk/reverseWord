public class reverseWord{

    public static void main(String[] args){
        String word = "Porcelanowy Sedes";
        String nword= "";
        char ch;

        for (int i = (word.length() - 1); i >= 0; i--){
            ch = word.charAt(i);
            nword += ch;
        }
        System.out.println(nword.toLowerCase());
        
    }
}
